{
  "name": "@githubnext/github-copilot-cli (via npm)",
  "id": "github-copilot-cli",
  "version": "1.1.0",
  "description": "Install the @githubnext/github-copilot-cli package via NPM registry",
  "options": {
    "version": {
      "type": "string",
      "proposals": ["latest"],
      "default": "latest",
      "description": "Select or enter a @githubnext/github-copilot-cli version."
    },
    "helper": {
      "type": "boolean",
      "default": false,
      "description": "Install `helper-gh-copilot-cli.sh`."
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/devcontainers/features/node"
  ]
}
